{
  "name": "sp17-result",
  "version": "2023.2.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "complete": "run-s dev build test",
    "dev": "run-s dev:*",
    "dev:nim": "nim js -d:release index",
    "dev:css": "tailwindcss build style.postcss -o style_dev.css",
    "dev:css-minify": "cleancss -o style.min.css style_dev.css",
    "build": "run-s build:*",
    "build:minify": "terser -c passes=10,unsafe,drop_console,hoist_vars,hoist_funs=true -m --toplevel -ecma 2022 -o index.js -- index.js",
    "test": "run-s test:*",
    "test:display": "opener ./index.html"
  },
  "author": "hand_accident",
  "license": "WTFPL",
  "dependencies": {
    "clean-css-cli": "^5.6.1",
    "daisyui": "^2.45.0",
    "npm-run-all": "^4.1.5",
    "tailwind": "^4.0.0",
    "terser": "^5.16.1"
  },
  "devDependencies": {
    "opener": "^1.5.2"
  }
}